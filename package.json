{
  "name": "quick-remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build --sourcemap",
    "build:server": "tsx ./scripts/build-server.ts",
    "dev": "remix dev -c \"node ./server/dev-server.js\" --manual",
    "start": "cross-env NODE_ENV=production node start.js",
    "typecheck": "tsc",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|css|ts|tsx|cjs)\"",
    "format": "npm run prettier -- --list-different",
    "format:write": "npm run prettier -- --write",
    "lint": "eslint --ignore-path .gitignore --ext js,jsx,ts,tsx,cjs .",
    "validate": "run-s typecheck lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@remix-run/css-bundle": "^2.2.0",
    "@remix-run/express": "^2.2.0",
    "@remix-run/node": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "address": "^2.0.1",
    "close-with-grace": "^1.2.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.4",
    "glob": "^10.3.10",
    "isbot": "^3.6.8",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.2.0",
    "@remix-run/eslint-config": "^2.2.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.4",
    "@types/morgan": "^1.9.4",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/source-map-support": "^0.5.6",
    "autoprefixer": "^10.4.16",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "esbuild": "^0.19.5",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.1.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": "18"
  }
}
